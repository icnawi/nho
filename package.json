{
  "name": "rhombus",
  "version": "0.1.0",
  "description": "",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "pnpm build --watch",
    "copy:env": "cp .env.example .env",
    "watch": "nodemon --watch './**/*.ts' --exec 'ts-node src/main.ts'",
    "dev": "tsc-watch --onSuccess \"npm run watch\"",
    "lint:check": "eslint --cache \"src/**/*.ts\"",
    "lint:fix": "pnpm lint:check --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "pnpm test -- --findRelatedTests",
    "test:ci": "pnpm test -- --coverage",
    "prettier:check": "prettier src --check",
    "prettier:fix": "prettier -- --write",
    "prettier": "prettier './src/**/*.{js,ts}'"
  },
  "keywords": [
    "solana transaction events parser"
  ],
  "author": "Eugene Karakulin",
  "license": "ISC",
  "dependencies": {
    "@blockworks-foundation/mango-client": "^3.6.20",
    "@fastify/redis": "^6.1.1",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.0",
    "@fastify/type-provider-typebox": "^3.1.0",
    "@project-serum/anchor": "^0.26.0",
    "@sinclair/typebox": "^0.27.8",
    "@solana/web3.js": "^1.75.0",
    "ajv": "^8.12.0",
    "bn.js": "^5.2.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-cors": "^6.1.0",
    "fastify-helmet": "^7.1.0",
    "fastify-plugin": "^4.5.0",
    "mongodb": "^5.3.0",
    "mongoose": "^7.0.4",
    "ramda": "^0.29.0",
    "rxjs": "^7.8.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.12",
    "@types/ramda": "^0.29.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm lint:fix",
      "pnpm run test:staged"
    ]
  }
}
